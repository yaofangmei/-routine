<view class='container'>
  <!------------- section1 start------------- -->
  <view class='section1'>
    <view class='login'>
      <image src="../../images/login.png"></image>
    </view>
  </view>
  <!------------- section2 start------------- -->
  <view class='section2'>
    <view class='content'>
      <view class='phoneNumber'>手机号</view>
      <input placeholder='请填写手机号' class='input'></input>
      <text class='achieve' bindtap='onidentifyCode'>获取验证码</text>
    </view>
  </view> 
  <!------------- section3 start------------- -->
  <view class='section3'>
      <view class='content'>
        <view class='identifyCode'>验证码</view>
        <input placeholder='请填写验证码' class='input'></input>
      </view>
  </view>
  <!------------- section4 start------------- -->
  <view class='section4'>
    <button type='primary' disabled='disabled'>登陆</button>
  </view>
  

</view>